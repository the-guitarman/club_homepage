<div class="row">
  <div class="col-xs-12 col-sm-8 col-sm-offset-2">
    <div class="jumbotron content-box">
      <h2><%= gettext("create_new_team_image") %></h2>

      <%= render "form.html", changeset: @changeset,
                 action: Routes.team_image_path(@conn, :create),
                 team_options: @team_options
      %>

      <%= link gettext("all") <> " " <> gettext("team_images"), to: Routes.team_image_path(@conn, :index) %>
    </div>
  </div>
</div>
